{
  "name": "hashpass.tech",
  "main": "expo-router/entry",
  "version": "1.6.0",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=4096' expo start",
    "start:reset": "NODE_OPTIONS='--max-old-space-size=4096' expo start --clear",
    "android": "NODE_OPTIONS='--max-old-space-size=4096' expo start --android",
    "ios": "NODE_OPTIONS='--max-old-space-size=4096' expo start --ios",
    "web": "NODE_OPTIONS='--max-old-space-size=4096' expo start --web --clear",
    "test": "jest --watchAll",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' expo start --web --clear",
    "build:web": "npm run prebuild && expo export -p web && npm run postbuild:web && npx workbox-cli generateSW workbox-config.js",
    "postbuild:web": "if [ -d dist/server/_expo ]; then mkdir -p dist/client/_expo && cp -r dist/server/_expo/* dist/client/_expo/; fi && if [ -d dist/server/static ]; then mkdir -p dist/client/static && cp -r dist/server/static/* dist/client/static/; fi",
    "prebuild": "rm -rf dist",
    "lint": "expo lint",
    "deploy": "npm run build:web && netlify deploy --prod",
    "seed:bslatam": "node ./scripts/seed-bslatam.mjs ./speakers.json",
    "test:e2e:bslatam": "node ./tests/bslatam/e2e.mjs",
    "amplify:push": "amplify push",
    "amplify:deploy": "amplify publish",
    "version:update": "node scripts/update-version.mjs",
    "version:bump": "node scripts/version-bump.js",
    "parameters:setup": "./scripts/setup-parameters.sh create",
    "parameters:verify": "./scripts/setup-parameters.sh verify",
    "parameters:list": "./scripts/setup-parameters.sh list",
    "lambda:test": "aws lambda invoke --function-name agendaMonitor --payload '{\"force\": true}' response.json && cat response.json",
    "deploy:amplify": "npm run build:web && npm run amplify:deploy",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile"
  },
  "expo": {
    "extra": {
      "EXPO_PUBLIC_API_BASE_URL": "https://hashpass.co/api"
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@adraffy/ens-normalize": "^1.11.1",
    "@expo/vector-icons": "^14.1.0",
    "@expo/webpack-config": "^19.0.1",
    "@lingui/babel-plugin-lingui-macro": "^5.5.2",
    "@lingui/core": "^5.5.2",
    "@lingui/format-json": "^5.5.2",
