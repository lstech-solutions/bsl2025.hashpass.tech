{
  "name": "hashpass.tech",
  "main": "expo-router/entry",
  "version": "1.6.94",
  "scripts": {
    "start": "NODE_OPTIONS='--max-old-space-size=4096' expo start",
    "start:reset": "NODE_OPTIONS='--max-old-space-size=4096' expo start --clear",
    "android": "NODE_OPTIONS='--max-old-space-size=4096' expo start --android",
    "ios": "NODE_OPTIONS='--max-old-space-size=4096' expo start --ios",
    "web": "NODE_OPTIONS='--max-old-space-size=4096' expo start --web --clear",
    "test": "jest --watchAll",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' expo start --web --clear",
    "build:web": "npm run prebuild && node scripts/update-sw-version.mjs && expo export -p web && npm run postbuild:web && (npm run build:storybook || echo 'Storybook build skipped') && (npx workbox-cli generateSW workbox-config.js || echo 'Workbox generation failed, continuing without service worker')",
    "build:storybook": "storybook build -o storybook-static || echo 'Storybook not available, skipping build'",
    "postbuild:web": "if [ -d dist/server/_expo ]; then mkdir -p dist/client/_expo && cp -r dist/server/_expo/* dist/client/_expo/; fi && if [ -d dist/server/static ]; then mkdir -p dist/client/static && cp -r dist/server/static/* dist/client/static/; fi && if [ -d storybook-static ]; then mkdir -p dist/client/storybook && cp -r storybook-static/* dist/client/storybook/; else echo 'Storybook static directory not found, skipping copy'; fi && mkdir -p dist/client/config && cp config/versions.json dist/client/config/versions.json && cp public/sw.js dist/client/sw.js",
    "prebuild": "rm -rf dist",
    "lint": "expo lint",
    "deploy": "npm run build:web && netlify deploy --prod",
    "seed:bslatam": "node ./scripts/seed-bslatam.mjs ./speakers.json",
    "test:e2e:bslatam": "node ./tests/bslatam/e2e.mjs",
    "match:speakers": "node ./scripts/match-users-to-speakers-update-vip.mjs",
    "grant:lukas": "node ./scripts/grant-1-luka-to-all-users.mjs",
    "amplify:push": "amplify push",
    "amplify:deploy": "amplify publish",
    "version:update": "node scripts/update-version.mjs",
    "version:bump": "node scripts/version-bump.js",
    "parameters:setup": "./scripts/setup-parameters.sh create",
    "parameters:verify": "./scripts/setup-parameters.sh verify",
    "parameters:list": "./scripts/setup-parameters.sh list",
    "lambda:test": "aws lambda invoke --function-name agendaMonitor --payload '{\"force\": true}' response.json && cat response.json",
    "deploy:amplify": "npm run build:web && npm run amplify:deploy",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "upload:email-assets": "node scripts/upload-email-assets-to-s3.js",
    "screenshots:email": "node scripts/capture-email-screenshots.js",
    "screenshots:speaker": "node scripts/capture-speaker-screenshots.js",
    "test:all-emails": "node scripts/test-all-emails.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static",
    "docs:dev": "npm run storybook",
    "docs:build": "npm run build-storybook",
    "docs:serve": "npx serve storybook-static",
    "docs:deploy:netlify": "npm run build-storybook && netlify deploy --prod --dir=storybook-static",
    "prepare": "node -e \"if (process.env.CI || process.env.NPM_CONFIG_PRODUCTION || process.env.NODE_ENV === 'production') { process.exit(0); } try { require.resolve('husky'); } catch (e) { if (e.code === 'MODULE_NOT_FOUND') process.exit(0); throw e; }\""
  },
  "expo": {
    "extra": {
      "EXPO_PUBLIC_API_BASE_URL": "https://hashpass.co/api"
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
